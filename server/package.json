{
  "name": "web-website-server",
  "version": "1.0.0",
  "description": "Backend server for web website",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts --clear-screen=false",
    "build": "tsc --build --verbose",
    "build:watch": "tsc --build --watch",
    "test": "jest",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "vite": "7.1.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.8.2",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2"
  }
}
